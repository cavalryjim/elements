[<% @elements.each_with_index do |element,i| %>
{
  "attr":
    {
      "id":"node_<%= element.id %>",
      "rel":"<%= element.attachable.blank? ? (element.is_root? ? "drive" : "element") : element.attachable.type %>"
    },
  "data":"<%=h element.name %>"
  <%= ',"state":"closed"' if element.has_children? %>
}<%= "," if i < @elements.count-1 %>
<% end %>]

